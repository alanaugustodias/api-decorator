{
    "name": "api-decorator-example",
    "version": "1.0.0",
    "description": "Node API Decorator Example",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist/*",
        "build": "npm run clean && tsc",
        "compile": "npm run build && node dist/index.js",
        "dev": "npm run build && nodemon dist/index.js",
        "dev:watch": "nodemon -e ts --exec \"npm run compile\""
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:alanaugustodias/api-decorator.git"
    },
    "keywords": [
        "api-decorator",
        "annotations",
        "api",
        "rest",
        "restful"
    ],
    "author": "Alan Dias",
    "contributors": [
        "@alanaugustodias"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alanaugustodias/api-decorator/issues"
    },
    "homepage": "https://github.com/alanaugustodias/api-decorator/blob/main/README.md",
    "engines": {
        "node": ">=10.0.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.10.1",
        "@babel/preset-env": "^7.10.3",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.31",
        "nodemon": "^2.0.7",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "api-decorator": "0.1.0-rc.6",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1"
    }
}
